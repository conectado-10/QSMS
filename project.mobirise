inline title=\"MM/SM BoxShadow Inner Opacity\" name=\"MenuBoxShadowInnerOpacity\" min=\"0.0\" max=\"1.0\" step=\"0.1\" value=\"1\" condition=\"MenuBoxShadow || SMBoxShadow\">\n        <input type=\"color\" inline title=\"MM/SM BoxShadow Outer\" name=\"MenuBoxShadowOuterColor\" value=\"#232323\" condition=\"MenuBoxShadow || SMBoxShadow\">\n        <input type=\"range\" inline title=\"MM/SM BoxShadow Outer Opacity\" name=\"MenuBoxShadowOuterOpacity\" min=\"0.0\" max=\"1.0\" step=\"0.1\" value=\"0.4\" condition=\"MenuBoxShadow || SMBoxShadoww\">\n        <header>Sub-Menu (SM) Config</header>\n        <input type=\"color\" title=\"SM BG Color\" name=\"dropdownBg\" value=\"#ffffff\">\n        <input type=\"range\" inline title=\"SM BG Opacity\" name=\"SubMenuBgOpacity\" min=\"0.3\" max=\"1\" step=\"0.05\" value=\"0.9\">\n        <input type=\"range\" inline title=\"SM BG Hover Darken %\" name=\"SubMenuHoverDarkenPercent\" min=\"1\" max=\"100\" step=\"1\" value=\"25\">\n        <input type=\"checkbox\" title=\"SM Item Hover\" name=\"SubMenuHover\" checked>\n        <input type=\"color\" title=\"SM Item Hover Color\" name=\"dropdownItemsHover\" value=\"#cc2952\" condition=\"SubMenuHover\">\n        <input type=\"range\" inline title=\"SM Top Padding\" name=\"SubMenuTopPadding\" min=\"0\" max=\"5\" step=\"0.05\" value=\"1.6\">\n        <input type=\"color\" title=\"SM Underline Color\" name=\"SubMenuLineColor\" value=\"#767676\">\n        <input type=\"range\" inline title=\"SM Underline Thickness\" name=\"SubMenuUnderlineThicknessPX\" min=\"0\" max=\"10\" step=\".25\" value=\"0.5\">\n        <input type=\"range\" inline title=\"SM Rounded Corners\" name=\"SubMenuRoundedCorners\" min=\"0.0\" max=\"1.5\" step=\"0.1\" value=\"0\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n\n    <nav class=\"navbar navbar-dropdown TopMenuUnderline\" mbr-class=\"{'navbar-fixed-top':sticky,\n                     'navbar-expand-lg':!collapsed,\n                     'collapsed